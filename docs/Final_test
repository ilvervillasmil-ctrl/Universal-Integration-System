1. import math
2. from typing import List, Dict
3. from functools import wraps
4. from core.constants import ALPHA, BETA, S_REF, get_layer_frequency, PHI
5.
6. # Custom Exception for L6 Purpose Alignment
7. class PurposeAlignmentError(Exception):
8. """Exception raised when L6 friction (phi) is not 0.0"""
9. pass
10.
11. def validate_L6_friction(func):
12. """
13. Decorator to validate that L6 (Purpose layer) has friction = 0.0
14. Raises PurposeAlignmentError if L6 friction is not 0.0
15. """
16. @wraps(func)
17. def wrapper(self, layers_data: List[Dict[str, float]], *args, **kwargs):
18. # L6 is the last layer (index 6)
19. if len(layers_data) >= 7:
20. l6_friction = layers_data[6].get('phi', 0.0)
21. if l6_friction != 0.0:
22. raise PurposeAlignmentError(
23. f"L6 Purpose layer must have friction (phi) = 0.0, but got {l6_friction}"
24. )
25. return func(self, layers_data, *args, **kwargs)
26. return wrapper
27.
28.
29. class OmegaEngine:
30. def __init__(self):
31. self.version = "2.6.6"
32.
33. def calculate_presence(self, dispersion: float) -> float:
34. """Pt = 1 / (1 + dispersion)"""
35. return 1 / (1 + abs(dispersion))
36.
37. def calculate_wonder(self, novelty: float, sensitivity: float = 1.0) -> float:
38. """A = 1 - e^(-N/k)"""
39. if sensitivity <= 0: sensitivity = 1.0
40. return 1 - math.exp(-abs(novelty) / sensitivity)
41.
42. def calculate_layer_energy(self, L: float, phi: float, layer_index: int) -> float:
43. """Ei = Li * (1 - phi) * frequency_i"""
44. frequency = get_layer_frequency(layer_index)
45. return L * (1 - phi) * frequency
46.
47. def calculate_external_coherence(self, C1: float, C2: float, theta: float) -> float:
48. """
49. I_ext = √(C₁² + C₂² + 2·C₁·C₂·cos(θ))
50. External coherence dependent on phase
51. """
52. cos_theta = math.cos(math.radians(theta))
53. i_ext_squared = C1**2 + C2**2 + 2 * C1 * C2 * cos_theta
54. return math.sqrt(max(0, i_ext_squared)) # Ensure non-negative
55.
56. def calculate_harmony(self, entropy: float, s_max: float = 1.0) -> float:
57. """
58. H(S) = 1 - S/S_max
59. Harmony as a function of entropy
60. """
61. if s_max == 0:
62. return 0.0
63. return max(0, 1 - (entropy / s_max))
64.
65. @validate_L6_friction
66. def compute_coherence(
67. self,
68. layers_data: List[Dict[str, float]],
69. dispersion: float = 0.0,
70. novelty: float = 0.5,
71. C1: float = 0.5,
72. C2: float = 0.5,
73. theta: float = 0.0,
74. s_max: float = None
75. ) -> float:
76. """
77. Calculates C_Ω using the Villasmil-Ω advanced formula:
78. C_Ω = α·H(S) + β·I_ext
79.
80. Where:
81. - H(S) = 1 - S/S_max (harmony based on entropy)
82. - I_ext = √(C₁² + C₂² + 2·C₁·C₂·cos(θ)) (external coherence)
83. - Golden ratio (PHI) is used as a scaling factor
84.
85. The coherence is scaled by PHI for universal alignment.
86. """
87. # 1. Calculate Energies and Total Energy
88. energies = []
89. for i, data in enumerate(layers_data):
90. e_i = self.calculate_layer_energy(data['L'], data['phi'], i)
91. energies.append(e_i)
92.
93. total_energy = sum(energies)
94. if total_energy == 0: return 0.0
95.
96. # 2. Entropy calculation (Shannon Entropy)
97. entropy = 0.0
98. for e_i in energies:
99. p_i = e_i / total_energy
100. if p_i > 0:
101. entropy -= p_i * math.log(p_i, 7) # Log base 7 for 7 layers
102.
103. # 3. Calculate Harmony H(S) using the new formula
104. # Use S_REF as s_max if not provided
105. if s_max is None:
106. s_max = S_REF
107. harmony = self.calculate_harmony(entropy, s_max)
108.
109. # 4. Calculate External Coherence I_ext with phase dependency
110. i_ext = self.calculate_external_coherence(C1, C2, theta)
111.
112. # 5. Apply Villasmil-Ω Master Formula
113. # C_Ω = α·H(S) + β·I_ext
114. c_omega = (ALPHA * harmony) + (BETA * i_ext)
115.
116. # 6. Scale with Golden Ratio (PHI) for universal alignment
117. # PHI acts as a harmonic scaling factor to align layers with universal proportions
118. c_omega_scaled = c_omega * (PHI / 2.0) # Scale by PHI/2 to keep values reasonable
119.
120. return min(c_omega_scaled, 1.0) # Clamping at 1.0 max
121.
